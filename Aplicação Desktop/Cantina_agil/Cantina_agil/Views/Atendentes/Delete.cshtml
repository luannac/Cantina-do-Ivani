@model Cantina_agil.Models.Atendente

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Deletar</h2>

<h3>Tem certeza que deseja Deletar?</h3>
<div>
    <h4>Atendentes</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.nomeAtendente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nomeAtendente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.loginAtendente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.loginAtendente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.senhaAtendente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.senhaAtendente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.emailAtendente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.emailAtendente)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.cpf)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cpf)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" />
            <a href=@Url.Action("Index","Atendentes") class="btn btn-primary" data-toggle="button">
                Voltar para Lista
            </a> 
        </div>
    }
</div>